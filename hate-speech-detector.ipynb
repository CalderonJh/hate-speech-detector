        "\n",
        "from sklearn.model_selection import *\n",
        "from sklearn.naive_bayes import GaussianNB\n",
        "from sklearn.metrics import *\n",
        "\n",
        "model = GaussianNB()\n",
        "model.fit(Reduced_X_train, y_train)"
      ]
